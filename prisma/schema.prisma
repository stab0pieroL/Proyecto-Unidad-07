// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String  @unique
  password  String
  email String  @unique
}

model Playlist {
  id        Int     @id @default(autoincrement())
  title     String
  User_id   User    @relation(fields: [], references: [id])

}